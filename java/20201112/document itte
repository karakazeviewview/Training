 import java.util.*;
  2 public class Random300{
  3   public static void main(String[] args){
  4     int[] nums1=new int[5];
  5     for(int i=0;i<nums1.length;i++){
  6       nums1[i]=new Random().nextInt(100)+1;
  7     //int[] nums2=new int[5];
  8     //nums2=nums1*3;
  9       }
 10       for(int n=0;n<5;n++){
 11         if(n==0){
 12           System.out.println("***nums1***");
 13         }
 14           System.out.println(nums1[n]);
 15         //System.out.println(Arrays.toString(nums2));
 16         }
 17       int[] nums2=new int[5];
 18       for(int j=0;j<5;j++){
 19         nums2[j]=nums1[j]*3;
 20         if(j==0){
 21           System.out.println("***nums2***");
 22         }
 23         System.out.println(nums2[j]);
 24       }
 25   }
 26 }
