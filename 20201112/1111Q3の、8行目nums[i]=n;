1 import java.util.*;
  2 public class Ichiran{
  3   public static void main(String[] args){
  4     System.out.print("何人>");
  5     int zenin=new java.util.Scanner(System.in).nextInt();
  6     int[] score=new int[zenin];
  7     for(int i=0;i<zenin;i++){
  8       System.out.print((i+1)+"人目の点数>");
  9       score[i] =new java.util.Scanner(System.in).nextInt();
 10       }
 11       System.out.println(Arrays.toString(score));
 12   }
 13 }
